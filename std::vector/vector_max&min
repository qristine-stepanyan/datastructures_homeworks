#include <iostream>

int max_element(int* begin,int* end){
  int max= *begin;
    for(int* current = begin;current != end;++current){
        if(*current > max){
          max = *current;
        }
    }
return max;
}
int min_element(int* begin,int* end){
  int min = *begin; 
    for(int* current = begin;current != end;++current){
        if(*current < min){
          min = *current;
        }
    }
return min;
}
int main(){
    int n;
    std::cin >>n;
    int arr[n];
    for(int i = 0;i < n;++i){
        std::cin>> arr[i];
    }
    int* begin = &arr[0];
    int* end = &arr[n];
    int max = max_element(begin,end);
    int min = min_element(begin,end);
    std::cout<< "Max el " <<max<<"\n";
    std::cout<< "Min el " <<min;
return 0;
}
